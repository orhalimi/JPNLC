{
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
          "jsx": true
      }
  },
    "rules": {
      "jsx-quotes": [2, "prefer-single"],
      "react/jsx-curly-spacing": [2, "always"],
      "react/prefer-stateless-function": [0],
      "react/jsx-indent": [2, 2]
    },
    "plugins": [
        "react"
    ]
  }