{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": "last 4 versions"
        },
        "loose": true,
        "modules": false
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": ["babel-plugin-transform-es2015-modules-commonjs"]
    },
    "production":{
      "plugins": ["babel-plugin-transform-es2015-modules-commonjs"]
    }
  }
}
